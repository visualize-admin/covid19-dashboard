{
  "name": "@c19/client",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "author": "shiftcode GmbH <team@shiftcode.ch>",
  "contributors": [
    {
      "name": "Simon Mumenthaler",
      "email": "simon.mumenthaler@shiftcode.ch"
    },
    {
      "name": "Simon Guggisberg",
      "email": "simon.guggisberg@shiftcode.ch"
    }
  ],
  "workspaces": {
    "nohoist": [
      "@angular/**",
      "@angular-devkit/**",
      "codelyzer",
      "@nguniversal/**",
      "@types/**",
      "@shiftcode/ngx-aws",
      "@shiftcode/ngx-components",
      "@shiftcode/ngx-core",
      "@shiftcode/ngx-ssr"
    ]
  },
  "scripts": {
    "build": "yarn build:app",
    "build:app": "ng run client:browser:production && ng run client:server:production",
    "build:ci": "rm -rf ./dist && node ./export-base-runtime-config && yarn build:app",
    "licenses:dev": "yarn license-checker --development --csv --out ../../architecture/licenses/licenses-client-dev.csv --customPath ../../architecture/licenses/licenses-format.json",
    "licenses:prod": "yarn license-checker --production --csv --out ../../architecture/licenses/licenses-client-prod.csv --customPath ../../architecture/licenses/licenses-format.json",
    "licenses:update": "yarn licenses:prod && yarn licenses:dev",
    "lint": "yarn lint:app --fix && yarn lint:server --fix && yarn lint:tests --fix",
    "lint:app": "tslint --project ./tsconfig.app.json",
    "lint:ci": "yarn lint:app && yarn lint:tests && yarn lint:server",
    "lint:server": "tslint --project ./tsconfig.server.json",
    "lint:tests": "tslint --project ./tsconfig.spec.json",
    "optimizeSvg": "svgo --disable removeViewBox",
    "precommit": "lint-staged",
    "prettier": "yarn prettier:base 'src/**/*.{ts,html,scss}'",
    "prettier:base": "prettier --write --config ../../.prettierrc.yml",
    "prettier:staged": "yarn prettier:base",
    "start": "ng run client:serve-browser:development",
    "start:ssr": "ng run client:serve-ssr:development",
    "test": "jest",
    "test:ci": "jest"
  },
  "dependencies": {
    "@angular/animations": "~13.1.3",
    "@angular/cdk": "~13.1.3",
    "@angular/common": "~13.1.3",
    "@angular/compiler": "~13.1.3",
    "@angular/core": "~13.1.3",
    "@angular/forms": "~13.1.3",
    "@angular/platform-browser": "~13.1.3",
    "@angular/platform-browser-dynamic": "~13.1.3",
    "@angular/platform-server": "~13.1.3",
    "@angular/router": "~13.1.3",
    "@aws-sdk/client-cloudwatch-logs": "^3.47.0",
    "@aws-sdk/types": "^3.47.0",
    "@c19/commons": "*",
    "@juggle/resize-observer": "^3.2.0",
    "@nguniversal/common": "~13.0.2",
    "@nguniversal/express-engine": "~13.0.2",
    "@shiftcode/ngx-aws": "^1.0.0",
    "@shiftcode/ngx-components": "^1.0.0",
    "@shiftcode/ngx-core": "^1.0.0",
    "@shiftcode/ngx-ssr": "^1.0.0",
    "@shiftcode/utilities": "^1.0.0",
    "ansi-styles": "^6.1.0",
    "aws-serverless-express": "^3.3.8",
    "change-case": "^4.1.1",
    "d3": "^6.2.0",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "express": "^4.17.1",
    "just-snake-case": "^3.0.1",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.3",
    "@nguniversal/builders": "~13.0.2",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/d3": "^5.16.3",
    "@types/express": "4.17.8",
    "@types/resize-observer-browser": "^0.1.6",
    "codelyzer": "^6.0.2",
    "jest-preset-angular": "^11.0.1",
    "svgo": "^1.3.2",
    "typescript": "~4.5.5"
  }
}
